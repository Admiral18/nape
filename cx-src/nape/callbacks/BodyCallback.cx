package nape.callbacks;
$(import);

class BodyCallback extends Callback {
    public function new() {
        super();
    }

    property(body, Body, {
        /*return*/ pr(inner).body.outer;
    })

    @:$keep public override function toString() {
        var ret = "Cb:";
        ret += ["WAKE","SLEEP"][pr(inner).event-Flag(CbEvent,WAKE)];
        ret += ":"+body.toString();
        ret += " : listener: "+listener;
        return ret;
    }
}
