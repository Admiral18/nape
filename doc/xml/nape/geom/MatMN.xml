<?xml version="1.0" encoding="utf-8" ?>
<data>

<class package="nape.geom" name="MatMN">
	<file>MatMN.hx</file>
	<description>
		<text>General matrix class.</text>
	</description>
	<short>
		General matrix class.
	</short>
	
	<property name="rows" type="Int" readonly="true">
		<description>Number of rows</description>
		<detail>
			<description>
				Number of rows in matrix
			</description>
		</detail>
	</property>
	<property name="cols" type="Int" readonly="true">
		<description>Number of columns</description>
		<detail>
			<description>
				Number of columns in matrix
			</description>
		</detail>
	</property>

	<constructor>
		<arg name="rows" type="Int"/>
		<arg name="cols" type="Int"/>
		<description>
			Construct a new matrix.
		</description>
		<detail>
			<description>
				Construct a new matrix of given dimension.
			</description>
		</detail>
	</constructor>
	
	<method name="x" const="true" return="Float">
		<arg name="row" type="Int"/>
		<arg name="col" type="Int"/>
		<description>Return matrix element</description>
		<detail>
			<description>Return matrix element</description>
		</detail>
	</method>
	
	<method name="setx" return="Float">
		<arg name="row" type="Int"/>
		<arg name="col" type="Int"/>
		<arg name="value" type="Float"/>
		<description>Set and return matrix element</description>
		<detail>
			<description>Set and return matrix element</description>
		</detail>
	</method>
	
	<method/>
	<method name="transpose" const="true" return="MatMN">
		<description>Return transposed matrix</description>
		<detail>
			<description>Return transposition of this matrix</description>
		</detail>
	</method>
	
	<method name="mul" const="true" return="MatMN">
		<description>Multiply matrices</description>
		<arg name="y" type="MatMN" const="true"/>
		<detail>
			<description>Multiply matrices returning result</description>
		</detail>
	</method>
	
</class>
	
</data>